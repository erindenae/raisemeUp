{
  "extends": [
    "eslint-config-airbnb",
    "prettier",
  ],
  "parser": "babel-eslint",
  "plugins": [
    "flowtype"
  ],
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
  },
  "settings": {
    "import/resolver": {
      "babel-module": {
      }
    },
    "import/core-modules": [
      "classnames",
      "prop-types",
      "react",
      "react-dom",
      "react-dom/server",
      "react-dom/test-utils",
      "react-helmet",
      "react-redux",
      "redux"
    ]
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prefer-arrow-callback": ["error", { "allowNamedFunctions": true }],
    "no-underscore-dangle": ["error", { "allow": ["__PRELOADED_STATE__", "__REDUX_DEVTOOLS_EXTENSION__"] }],
    "wrap-iife": [2, "any"],
    "react/no-did-mount-set-state": "off"
  }
}

